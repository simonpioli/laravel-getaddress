{
    "name": "szhorvath/laravel-getaddress",
    "type": "library",
    "description": "Laravel Package for getaddress.io",
    "keywords": [
        "getaddress",
        "laravel-getaddress",
        "laravel",
        "getaddress.io",
        "address lookup"
    ],
    "homepage": "https://github.com/szhorvath/laravel-getaddress",
    "license": "MIT",
    "authors": [
        {
            "name": "Sandor Horvath",
            "email": "z.sandor.horvath@gmail.com",
            "homepage": "https://github.com/szhorvath/laravel-getaddress",
            "role": "Developer"
        },
        {
            "name": "Simon Pioli",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/config": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Szhorvath\\GetAddress\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szhorvath\\GetAddress\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Szhorvath\\GetAddress\\GetAddressServiceProvider"
            ],
            "aliases": {
                "getaddress": "Szhorvath\\GetAddress\\Facades\\GetAddress"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
